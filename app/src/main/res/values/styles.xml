        <item name="android:layout_height">@dimen/input_height</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/input_text_size</item>
<!--        <item name="android:background">@drawable/code_edit_text_background</item>-->
    </style>

